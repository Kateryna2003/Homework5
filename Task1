package com.company;

public class Main {

    public static void main(String[] args) {
            int [] first = {5, 15, -10, 8, -2};
            int [] second = {54, 32, -28, -6, 4};
            int [] result = new int [first.length];
            String s ="";
    for(int i = 0; i < first.length; i++ ){
                result[i]=first[i]+second[i];
                System.out.print(result[i] +", ");
            }
    }
}
